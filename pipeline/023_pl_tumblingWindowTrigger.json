{
	"name": "023_pl_tumblingWindowTrigger",
	"properties": {
		"activities": [
			{
				"name": "Copy data from source to destination",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[techbros].[023_prc_loadCustomer]",
						"storedProcedureParameters": {
							"endDate": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.edate",
									"type": "Expression"
								}
							},
							"startDate": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.sdate",
									"type": "Expression"
								}
							}
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "CustomerId",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "CustomerId",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "FName",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "FName",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "LName",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "LName",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "SaleDate",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "SaleDate",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_023_sqlSource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_023_sqlDestination",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"edate": {
				"type": "string"
			},
			"sdate": {
				"type": "string"
			}
		},
		"folder": {
			"name": "023"
		},
		"annotations": []
	}
}