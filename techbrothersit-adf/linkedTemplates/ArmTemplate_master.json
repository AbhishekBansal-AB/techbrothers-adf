{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "techbrothersit-adf"
		},
		"blb_tbsource_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "tbstorage"
		},
		"lnkKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://techbrothers-keyvault.vault.azure.net/"
		},
		"lnkblobsourcelogs_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "tbstorage"
		},
		"techbrothersitdb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "tbsqlconnection"
		},
		"storageEvent_trigger_properties_022_pl_storageEventTrigger_parameters_fName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"storageEvent_trigger_properties_022_pl_storageEventTrigger_parameters_fPath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"storageEvent_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f8b296e1-7c62-474b-9317-cee7e5eb21dd/resourceGroups/techbrothersit-rg/providers/Microsoft.Storage/storageAccounts/techbrothersitstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "techbrothersit-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lnkKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('lnkKeyVault_properties_typeProperties_baseUrl')]"
					},
					"blb_tbsource_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('blb_tbsource_properties_typeProperties_connectionString_secretName')]"
					},
					"lnkblobsourcelogs_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('lnkblobsourcelogs_properties_typeProperties_connectionString_secretName')]"
					},
					"techbrothersitdb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('techbrothersitdb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "techbrothersit-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storageEvent_trigger_properties_022_pl_storageEventTrigger_parameters_fName": {
						"value": "[parameters('storageEvent_trigger_properties_022_pl_storageEventTrigger_parameters_fName')]"
					},
					"storageEvent_trigger_properties_022_pl_storageEventTrigger_parameters_fPath": {
						"value": "[parameters('storageEvent_trigger_properties_022_pl_storageEventTrigger_parameters_fPath')]"
					},
					"storageEvent_trigger_properties_typeProperties_scope": {
						"value": "[parameters('storageEvent_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'techbrothersit-adf_ArmTemplate_0')]"
			]
		}
	]
}